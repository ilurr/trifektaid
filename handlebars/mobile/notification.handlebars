<!DOCTYPE html>
<html lang="en">

{{>head font="link" mobile="true"}}

<body>
	<!-- s: root -->
	<div class="root">

		{{>header_mobile login=true active="Notification"}}
		
		<!-- grid -->
		<div class="grid">

			<div class="grid-flex">

				<div class="grid-col -nav">
					{{>nav_mobile active="Notification"}}
				</div>

				<div class="grid-col -wide">

					{{>title section="true" title="Notification"}}

					<!-- notification -->
					<div class="notify box">
						<div class="notify-block">
							{{>notify_list channel=people index_start=0 index_end=9}}
						</div>
						{{>more label="Load more"}}
					</div>

				</div>

			</div>

		</div>

	</div>
	<!-- e: root -->

{{>js mobile="true"}}
</body>
</html>
